<?xml version="1.0"?>

<!DOCTYPE scatterer>

<!-- weights: absorption, scattering, transmission -->
<homogeneous_scatterer mcweights="0, 1, 0">
  
  <KernelContainer average="yes">

    <E_Q_Kernel 
	E_Q="50" 
	S_Q="Q*Q*0.00276 * exp(-0.00276 * Q*Q)"
	Qmin="0./angstrom"
	Qmax="31./angstrom"
	absorption_coefficient="32.395/meter"
	scattering_coefficient="69.847/meter"
	/>

  </KernelContainer>

</homogeneous_scatterer>

