<?xml version="1.0"?>

<!DOCTYPE scatterer>

<!-- weights: absorption, scattering, transmission -->
<homogeneous_scatterer mcweights="0, 1, 0">
  
  <KernelContainer average="yes">

    <E_Q_Kernel 
	E_Q="Q*Q*2.76e-3" 
	S_Q="Q*Q*2.76e-3 * exp(-2.76e-3 * Q*Q)"
	Qmin="0./angstrom"
	Qmax="30./angstrom"
	/>
    
  </KernelContainer>

</homogeneous_scatterer>

